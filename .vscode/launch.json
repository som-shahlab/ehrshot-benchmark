                "--second_source_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/experiments/markdown_serialization_final/llm2vec_llama3_1_7b_instruct_supervised_unqiue_then_list_visits_wo_allconds_w_values_4k_no_labs_single_true_false_true_3_full_renamed",
                "--second_source_filter", "llm_llama,lr_lbfgs",
                "--output_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/experiments/markdown_serialization_final/gteqwen2_7b_instruct_unqiue_then_list_visits_wo_allconds_w_values_4k_no_labs_single_true_false_true_3_full_with_baselines_and_llama",
