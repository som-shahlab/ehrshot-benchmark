{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "Python Debugger: 3_consolidate_labels.py",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/ehrshot/3_consolidate_labels.py",
            "console": "integratedTerminal",
            "args": [
                "--path_to_labels_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/benchmark",
                "--is_force_refresh"
            ], 
        },

        {
            "name": "Python Debugger: 4 Generate AGR Features",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/ehrshot/4_generate_agr_features.py",
            "console": "integratedTerminal",
            "args": [
                "--path_to_database", "~/ehrshot-benchmark/EHRSHOT_ASSETS/femr/extract",
                "--path_to_labels_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/benchmark",
                "--path_to_features_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/features",
                "--num_threads", "20",
                "--is_force_refresh"
            ],

        },

        {
            "name": "Python Debugger: 4 Generate LLM Features",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/ehrshot/4_generate_llm_features.py",
            "console": "integratedTerminal",
            "args": [
                "--path_to_database", "~/ehrshot-benchmark/EHRSHOT_ASSETS/femr/extract",
                "--path_to_labels_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/benchmark",
                "--path_to_features_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/features",
                "--num_threads", "1",
                "--is_force_refresh",
                // "--text_encoder", "llm2vec_llama3_1_7b_instruct_supervised",
                // "--text_encoder", "llm2vec_llama3_1_7b_instruct_mimic_/mntp-supervised-mednli/Meta-Llama-3.1-8B-Instruct_1000_mntp_steps/MedNLI_train_m-Meta-Llama-3.1-8B-Instruct_l-512/checkpoint-200",
                // "--text_encoder", "gteqwen2_7b_instruct",
                // "--text_encoder", "gteqwen2_1_5b_instruct",
                "--text_encoder", "bioclinicalbert-fl-average-chunks",
                "--task_to_instructions", "~/ehrshot-benchmark/ehrshot/serialization/task_to_instructions.json",
                "--serialization_strategy", "list_visits_with_events",  // "list_events", "list_visits_with_events", ""demographics_with_aggregated_events"
                "--excluded_ontologies", "no_labs",
                "--unique_events", "true",
                "--numeric_values", "false",
                "--num_aggregated", "3",
                "--add_parent_concepts", "none"
            ],
        },

        { 
            "name": "Python Debugger: 7 Evaluate",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/ehrshot/7_eval.py",
            "console": "integratedTerminal",
            "args": [
                "--path_to_database", "~/ehrshot-benchmark/EHRSHOT_ASSETS/femr/extract",
                "--path_to_labels_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/benchmark",
                "--path_to_features_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/features",
                "--path_to_output_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/results",
                "--path_to_split_csv", "~/ehrshot-benchmark/EHRSHOT_ASSETS/splits/person_id_map.csv",
                "--labeling_function", "guo_los",
                "--shot_strat", "all",
                "--num_threads", "1"
            ]

        },

        {
            "name": "Python Debugger: 10_cis.py",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/ehrshot/10_cis.py",
            "console": "integratedTerminal",
            "args": [
                "--path_to_results_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/results"
            ], 
        }
    ]
}

