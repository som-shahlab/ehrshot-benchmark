                // "--serialization_strategy", "unique_then_list_visits_wo_allconds_w_values_4k_no_medications",
                // "--serialization_strategy", "unique_then_list_visits_wo_allconds_w_values_4k_no_procedures",
                "--serialization_strategy", "unique_then_list_visits_wo_allconds_w_values_4k",  // "unique_then_list_visits_wo_allconds_w_values_4k"
                "--excluded_ontologies", "no_labs_single",
                "--num_aggregated", "3",
            ],
        },

        { 
            "name": "Python Debugger: 7 Evaluate",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/ehrshot/7_eval.py",
            "console": "integratedTerminal",
            "args": [
                "--path_to_database", "~/ehrshot-benchmark/EHRSHOT_ASSETS/femr/extract",
                "--path_to_labels_dir", "~/ehrshot-benchmark/EHRSHOT_ASSETS/benchmark",
